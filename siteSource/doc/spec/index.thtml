<EBNFCDECL name='AdditionalChannelSection'/>
<EBNFCDECL name='AdditionalChannelSectionMark'/> = "[@>:]" | "[channel]" | "[chan]" | "[canal]" | "[cn]" | "[channel:]" | "[chan:]" | "[canal:]" | "[cn:]" .
</pre>

<h3 class='center-h' data-href="Clauses_section_chapter">
    Clauses section
</h3>

<LinkToContent/>	

<pre class='code'>
<EBNFCDECL name='ClausesSection'/>
<EBNFCDECL name='ClausesSectionMark'/> = "&:" | "where" | "w" | "where:" | "w:" .
<EBNFCDECL name='AdditionalClausesSection'/>
<EBNFCDECL name='AdditionalClausesSectionMark'/> = "[&:]" | "[where]" | "[w]" | "[where:]" | "[w:]" .
</pre>

<h3 class='center-h' data-href="No_contradict_section_chapter">
    No contradict section
</h3>

<LinkToContent/>	

<pre class='code'>
<EBNFCDECL name='NoContradictSection'/>
<EBNFCDECL name='NoContradictSectionMark'/> = "^:" .
<EBNFCDECL name='AdditionalNoContradictSection'/>
<EBNFCDECL name='AdditionalNoContradictSectionMark'/> = "[^:]" .
</pre>

<h3 class='center-h' data-href="Access_section_chapter">
    Access section
</h3>

<LinkToContent/>	

<pre class='code'>
<EBNFCDECL name='AccessSection'/>
<EBNFCDECL name='AccessSectionMark'/> = "!!:" | "access" | "permissions" | "allow" | "permit" | "a" | "access:" | "permissions:" | "allow:" | "permit:" | "a:" .
<EBNFCDECL name='AdditionalAccessSection'/>
<EBNFCDECL name='AdditionalAccessSectionMark'/> = "[!!:]" | "[access]" | "[permissions]" | "[allow]" | "[permit]" | "[a]" | "[access:]" | "[permissions:]" | "[allow:]" | "[permit:]" | "[a:]" .
</pre>

<h3 class='center-h' data-href="Reason_section_chapter">
    Reason section
</h3>

<LinkToContent/>	

<pre class='code'>
<EBNFCDECL name='ReasonSection'/>
<EBNFCDECL name='ReasonSectionMark'/> = "|-<:" | "reason" | "cause" | "reason:" | "cause:" .
<EBNFCDECL name='AdditionalReasonSection'/>
<EBNFCDECL name='AdditionalReasonSectionMark'/> = "[|-<:]" | "[reason]" | "[cause]" | "[reason:]" | "[cause:]" .
</pre>

<h3 class='center-h' data-href="Holder_section_chapter">
    Holder section
</h3>

<LinkToContent/>	

<pre class='code'>
<EBNFCDECL name='HolderDesc'/>
<EBNFCDECL name='HolderDescMark'/> = "#:" | "of" | "of:" .
</pre>

<h3 class='center-h' data-href="Module_section_chapter">
    Module section
</h3>

<LinkToContent/>	

<pre class='code'>
<EBNFCDECL name='ModuleDesc'/>
<EBNFCDECL name='ModuleDescMark'/> = "$:" | "module" | "module:" .
</pre>

<h2 class='center-h' data-href="Expressions_chapter">
    Expressions
</h2>

<LinkToContent/>

<pre class='code'>
<EBNFCDECL name='Expr'/> = <EBNFC name='BooleanTypeExpr'/> | <EBNFC name='NumericTypeExpr'/> | <EBNFC name='StringTypeExpr'/> .
</pre>

<h3 class='center-h' data-href="Operators_chapter">
    Operators
</h3>

<LinkToContent/>

<h4 class='center-h' data-href="Assignment_operators_chapter">
    Assignment operators
</h4>

<LinkToContent/>

<h2 class='center-h' data-href="Statements_chapter">
    Statements
</h2>

<LinkToContent/>

<pre class='code'>
<EBNFCDECL name='StatementsSet'/> = <EBNFC name='Statement'/> [ { <EBNFC name='Statement'/> } ] .
<EBNFCDECL name='Statement'/> = <EBNFC name='UsualScope'/> | <EBNFC name='ClausedScopeForStatements'/> .
</pre>

<h3 class='center-h' data-href="Scopes_chapter">
    Scopes
</h3>

<LinkToContent/>

<pre class='code'>
<EBNFCDECL name='UsualScope'/> = [ <EBNFC name='AnnotationsList'/> ] "{" [ <EBNFC name='StatementsSet'/> ] "}" [ <EBNFC name='ModalitiesSet'/> ] ";" .
<EBNFCDECL name='ClausedScopeClause'/>
<EBNFCDECL name='ClausedScopeHeader'/> = "width" "{" <EBNFC name='ClausedScopeClause'/> "}" .
<EBNFCDECL name='ClausedScopeForStatements'/> = [ <EBNFC name='AnnotationsList'/> ] <EBNFC name='ClausedScopeHeader'/> "def" "{" [ <EBNFC name='StatementsSet'/> ] "}" [ <EBNFC name='ModalitiesSet'/> ] ";".
</pre>

<h2 class='center-h' data-href="Source_code_file_chapter">
    Source code file
</h2>

<LinkToContent/>

<pre class='code'>
<EBNFCDECL name='SourceCodeFileContent'/> = [ <EBNFC name='NamespaceDeclsSet'/> ][ <EBNFC name='ClassDeclsSet'/> ] [ <EBNFC name='ApplicationDeclsSet'/> ] [ <EBNFC name='FunctionDeclsSet'/> ] [ <EBNFC name='FactDeclsSet'/> ] [ <EBNFC name='LogicalRuleDeclsSet'/> ] [ <EBNFC name='CallBlocksSet'/> ] 
                [ <EBNFC name='TypeAliaseDeclsSet'/> ] [ <EBNFC name='FuzzySetDeclsSet'/> ] [ <EBNFC name='StateDeclsSet'/> ] [ <EBNFC name='ProcessDeclsSet'/> ] [ <EBNFC name='RangeDeclsSet'/> ] [ <EBNFC name='ClausedScopeForSourceCodeFilesSet'/> ] .
<EBNFCDECL name='ClausedScopeForSourceCodeFilesSet'/> = <EBNFC name=''/> [ { <EBNFC name='ClausedScopeForSourceCodeFile'/> } ] .
<EBNFCDECL name='ClausedScopeForSourceCodeFile '/> = <EBNFC name='ClausedScopeHeader'/> "def" "{" [ <EBNFC name='SourceCodeFileContent'/> ] "}" .
</pre>

