<h3 class='center-h' data-href="Functions_chapter">
    Functions
</h3>

<LinkToContent/>

<pre class='code'>
<EBNFCDECL name='FunctionDeclsSet'/> = <EBNFC name='FunctionDecl'/> [{ <EBNFC name='FunctionDecl'/> }] .
<EBNFCDECL name='FunctionName'/> = <EBNFC name='identifier'/> | <EBNFC name='identifier_with_namespace'/> .
<EBNFCDECL name='FunctionSignature'/> = "(" <EBNFC name='FunctionParamsDeclaration'/> ")" ":" <EBNFC name='FunctionReturnDeclaration'/> .
<EBNFCDECL name='FunctionIdentifier'/> = <EBNFC name='FunctionSignature'/> | <EBNFC name='FunctionName'/> .
<EBNFCDECL name='FunctionDecl'/> = [ <EBNFC name='AnnotationsList'/> ] "func" <EBNFC name='FunctionName'/> <EBNFC name='FunctionSignature'/> [ "is" <EBNFC name='InheritanceDecl'/> ]
           "=>" "{" [ <EBNFC name='FunctionBody'/> ] "}" ] [ <EBNFC name='ModalitiesSet'/> ] ";" .
<EBNFCDECL name='AnonymousFunction'/>
<EBNFCDECL name='FunctionParamsSetDeclaration'/> = <EBNFC name='FunctionParamDeclaration'/> [{ "," <EBNFC name='FunctionParamDeclaration'/> }] .
<EBNFCDECL name='FunctionParamDeclaration'/>
<EBNFCDECL name='FunctionReturnDeclaration'/> = <EBNFC name='TypeIdentifier'/> .
<EBNFCDECL name='FunctionBody'/>FunctionBody</a> = <EBNFC name='StatementsSet'/> .
</pre>

<h3 class='center-h' data-href="Facts_and_logical_rules_chapter">
    Facts and logical rules
</h3>

<LinkToContent/>

<pre class='code'>
<EBNFCDECL name='FactDeclsSet'/> = <EBNFC name='FactDecl'/> [{ <EBNFC name='FactDecl'/> }] .
<EBNFCDECL name='LogicalRuleDeclsSet'/> = <EBNFC name='LogicalRuleDecl'/> [{ <EBNFC name='LogicalRuleDecl'/> }] .
<EBNFCDECL name='FactIdentifier'/>
<EBNFCDECL name='FactDecl'/>
<EBNFCDECL name='ClausedScopeForFactsSet'/> = <EBNFC name='ClausedScopeForFact'/> [ { <EBNFC name='ClausedScopeForFact'/> } ] .
<EBNFCDECL name='ClausedScopeForFact'/> = [ <EBNFC name='AnnotationsList'/> ] <EBNFC name='ClausedScopeHeader'/> "def" "{" [ <EBNFC name=''/> ] "}" [ <EBNFC name='ModalitiesSet'/> ] .
<EBNFCDECL name='LogicalRuleIdentifier'/>
<EBNFCDECL name='LogicalRuleDecl'/>
<EBNFCDECL name='ClausedScopeForLogicalRulesSet'/> = <EBNFC name=''/> [ { <EBNFC name='ClausedScopeForLogicalRule'/> } ] .
<EBNFCDECL name='ClausedScopeForLogicalRule'/> = [ <EBNFC name='AnnotationsList'/> ] <EBNFC name='ClausedScopeHeader'/> "def" "{" [ <EBNFC name=''/> ] "}" [ <EBNFC name='ModalitiesSet'/> ] .
<EBNFCDECL name='ImplicationSymbol'/> = "->" | "<-" | "<->" | "→" | "←" | "↔" .
<EBNFCDECL name='NotContradictSymbol'/> = "^:" .
<EBNFCDECL name='LogicalSentence'/>
<EBNFCDECL name='LogicalAnnotation'/> = "[:" ( <EBNFC name='FactDecl'/> | "{:" <EBNFC name='ClassName'/> ":}" ) ":]"
